{
"version": 2,
"builds": [
{
"src": "backend/run.py",
"use": "@vercel/python"
},
{
"src": "frontend/package.json",
"use": "@vercel/static-build",
"config": {
"distDir": "build",
"buildCommand": "yarn build && ls -la"
}
}
],
"routes": [
{ "src": "/api/(.*)", "dest": "/backend/run.py" },
{ "src": "/(.*)", "dest": "/frontend/$1" }
]
}